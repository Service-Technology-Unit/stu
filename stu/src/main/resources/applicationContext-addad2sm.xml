<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
            http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
            http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

    <context:property-placeholder location="classpath*:feed.properties"/>

    <bean id="application" class="edu.ucdavis.ucdhs.isweb.keymaster.batch.sm.ServiceManagerAddAd2sm">
		<property name="systemId" value="${feed.system.id}"/>
        <property name="fileName" value="${feed.output.file.name}"/>
        <property name="inputFile" value="${feed.input.file.name}"/>
        <property name="jdbcDriverName" value="${jdbc.driverClassName}"/>
        <property name="dbURL" value="${jdbc.url}"/>
        <property name="dbUser" value="${jdbc.username}"/>
        <property name="dbPassword" value="${jdbc.password}"/>
    </bean>

</beans>
